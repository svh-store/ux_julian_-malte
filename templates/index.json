    "custom_liquid_yCF49d": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"stats-container\">\n  <div class=\"stats-item\">\n    <div class=\"counter\" data-target=\"3500000\" data-plus=\"true\">0</div>\n    <div>zufriedene Kunden</div>\n  </div>\n  <div class=\"stats-item\">\n    <div class=\"counter\" data-target=\"2550500\" data-plus=\"true\">0</div>\n    <div>verkaufte Hüllen</div>\n  </div>\n  <div class=\"stats-item\">\n    <div class=\"counter\" data-target=\"475000\" data-plus=\"true\">0</div>\n    <div>verkaufte Schutzfolien</div>\n  </div>\n</div>\n\n<style>\n  .stats-container {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n  }\n\n  .stats-item {\n    flex: 1 1 200px;\n    min-width: 200px;\n    text-align: center;\n    margin-bottom: 0;\n  }\n\n  .counter {\n    font-size: 48px;\n    font-weight: bold;\n    color: #F19222;\n    line-height: 1;\n    transition: transform 0.3s ease;\n  }\n\n  .counter:hover {\n    transform: scale(1.2) \n  }\n\n  .stats-item div:last-child {\n    margin-top: 4px;\n    font-size: 16px;\n    line-height: 1.2;\n  }\n\n  @media (max-width: 768px) {\n    .stats-container {\n      flex-direction: column;\n      align-items: center;\n    }\n\n    .stats-item {\n      width: 100%;\n      max-width: 300px;\n    }\n\n    .stats-item:not(:last-child) {\n      margin-bottom: 8px;\n    }\n\n    .stats-item:last-child {\n      margin-bottom: 0;\n    }\n\n    .counter {\n      font-size: 36px;\n    }\n\n    .stats-item div:last-child {\n      font-size: 14px;\n      margin-top: 2px;\n    }\n  }\n</style>\n\n<script>\n  document.addEventListener(\"DOMContentLoaded\", () => {\n    const counters = document.querySelectorAll('.counter');\n\n    function animateCounter(counter) {\n      const target = +counter.dataset.target;\n      const hasPlus = counter.hasAttribute('data-plus');\n      let current = 0;\n      const increment = Math.ceil(target / 200);\n\n      const update = () => {\n        current = Math.min(current + increment, target);\n        counter.innerText = current.toLocaleString() + (hasPlus && current === target ? '+' : '');\n        if (current < target) setTimeout(update, 20);\n      };\n\n      update();\n    }\n\n    const observer = new IntersectionObserver(entries => {\n      entries.forEach(entry => {\n        if (entry.isIntersecting) {\n          animateCounter(entry.target);\n          observer.unobserve(entry.target);\n        }\n      });\n    }, { threshold: 0.5 });\n\n    counters.forEach(counter => observer.observe(counter));\n  });\n</script>",
