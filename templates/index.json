    "custom_liquid_cBDWBj": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<!DOCTYPE html>\n<html lang=\"de\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Animation mit Pfeil & Titel</title>\n  <style>\n    /* Gesamt-Container */\n    .animation-wrapper {\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      flex-direction: column;\n      width: 100%;\n      margin: 20px auto;\n      padding: 10px;\n      text-align: center;\n      position: relative;\n    }\n\n    /* Pfeil-Grafik */\n    .slide-image {\n      width: 15vw;\n      max-width: 100px;\n      height: auto;\n      opacity: 0;\n      transform: rotate(90deg) translateY(-30px);\n      transition: transform 0.5s ease-out, opacity 0.5s ease-out;\n    }\n\n    /* Titel, der aus dem Pfeil slidet */\n    .animated-title {\n      font-size: clamp(24px, 5vw, 60px);\n      font-weight: bold;\n      color: #333;\n      opacity: 0;\n      transform: translateY(-30px);\n      transition: transform 0.5s ease-out 0.5s, opacity 0.5s ease-out 0.5s;\n    }\n\n    /* Orangener Strich */\n    .animated-line {\n      width: 100%;\n      height: 4px;\n      background-color: orange;\n      margin-top: 10px;\n      opacity: 0;\n      transform: translateY(-20px);\n      transition: transform 0.5s ease-out 1s, opacity 0.5s ease-out 1s;\n    }\n\n    /* Animation aktivieren */\n    .animation-wrapper.active .slide-image {\n      opacity: 1;\n      transform: rotate(90deg) translateY(0);\n    }\n\n    .animation-wrapper.active .animated-title {\n      opacity: 1;\n      transform: translateY(0);\n    }\n\n    .animation-wrapper.active .animated-line {\n      opacity: 1;\n      transform: translateY(0);\n    }\n\n    /* Responsive Anpassungen */\n    @media (max-width: 1024px) {\n      .animated-line {\n        width: 80%;\n        max-width: 300px;\n      }\n    }\n\n    @media (max-width: 768px) {\n      .slide-image {\n        width: 20vw;\n        max-width: 80px;\n      }\n\n      .animated-title {\n        font-size: clamp(18px, 4vw, 50px);\n      }\n\n      .animated-line {\n        width: 90%;\n      }\n    }\n  </style>\n</head>\n<body>\n\n  <div class=\"animation-wrapper\" id=\"animationWrapper2\">\n    <img src=\"https://cdn.shopify.com/s/files/1/0816/6368/9043/files/arrowmodern.svg?v=1740055818\" class=\"slide-image\" alt=\"Pfeil\">\n    <h2 class=\"animated-title\">Entdecke unser Sortiment</h2>\n    <div class=\"animated-line\"></div>\n  </div>\n\n  <script>\n    document.addEventListener(\"DOMContentLoaded\", function() {\n      setTimeout(function() {\n        document.getElementById(\"animationWrapper2\").classList.add(\"active\");\n      }, 800);\n    });\n  </script>\n\n</body>\n</html>",
